---
import AppLayout from '../layouts/AppLayout.astro'
import Navbar from '../components/Navbar/index.astro'
import ContentLayout from '../layouts/ContentLayout.astro'
import Footer from '../components/Footer/index.astro'
import enLang from '../data/lang/en.json'
// import espLang from '../data/lang/esp.json'

const { navbarLinks, home, about, projects, contact, footer } = enLang
---
<AppLayout>
  <Navbar links={navbarLinks} />
  <ContentLayout home={home} about={about} projects={projects} contact={contact}/>
  <Footer text={footer.text} />
</AppLayout>